	<script type="text/javascript"><!--
		function offsetTop(pos) 
		{
			var x = pos.offsetTop, parent = pos.offsetParent;
				while(parent && !parent.cbe) {
					x += parent.offsetTop;
					parent = parent.offsetParent;
				}
			return x;
		}

		function offsetLeft(pos) 
		{
			var x = pos.offsetLeft, parent = pos.offsetParent;
				while(parent && !parent.cbe) {
					x += parent.offsetLeft;
					parent = parent.offsetParent;
				}
			return x;
		}

		function showPopup(hoveritem, hoverpopup)
		{
			hp = document.getElementById(hoverpopup);

			hp.style.top = offsetTop(hoveritem) + hoveritem.offsetHeight + 2 + "px";
			hp.style.left = offsetLeft(hoveritem) + "px";

			hp.style.display = "block";
		}

		function hidePopup(hoverpopup)
		{
			hp = document.getElementById(hoverpopup);
			hp.style.display = "none";	
		}
	--></script>Affiliation: Neutral<br />
