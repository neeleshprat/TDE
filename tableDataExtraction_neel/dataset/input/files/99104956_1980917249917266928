    function responseForgotPassword(responseText, statusText)  { 
      jQuery('#top_area_messages').animate({height: "show"}, 800);
      jQuery('#forgot_password_form_box').animate({height: "hide"}, 800);
      hideLoginForm();
      hideProfileForm();
      hideSearchForm();
    } 
    
    jQuery(document).ready(function() {   
        
        $('#forgot_password_form').submit(function() { 
             var options = { 
                target:        '#top_area_messages',   
                success:       responseForgotPassword  
             };     
            $(this).ajaxSubmit(options); 
            return false; 
        }); 
        
    });    
  </script>  

