<script type="text/javascript">
               
  function responseProfileFormValidation(responseText, statusText)  {  
    jQuery('.profile_response_box').animate({height: "show"}, 800);          
    hideLoginForm();
    hideSearchForm();
    hideMessages();
    return false;
  } 

  function refreshProfile() {
    jQuery('#profile_form_right_area').empty(); 
    jQuery.get(url+'user/refresh',
       { },
       function(data){                   
         jQuery('#profile_form_right_area').append(data);
       }
     ); 
     return false;
  }

  jQuery(document).ready(function() {                                  
                                         
            
     jQuery('.go_login2').click(function(){    
          displayLoginForm();      
          return false;
      });           
      
      jQuery('.back_to_search_button').click(function(){
          displaySearchForm();
      });                
      
      $('#profile_ajax_form').submit(function() {   
           var options = { 
              target:        '#static_area_for_profile_form',   
              success:       responseProfileFormValidation  
          }; 
          $(this).ajaxSubmit(options); 
          return false; 
      }); 
      
      jQuery('#profile_form_area .checkbox').click(function(){
        if(!jQuery(this).hasClass('active')) {
          jQuery(this).addClass('active');
          jQuery(this).parent().children('input').val(1);
        }
        else {
          jQuery(this).removeClass('active');
          jQuery(this).parent().children('input').val(0);
        }
        return false;
      });
      
      jQuery('img.error').each(function(){
        jQuery(this).mouseover(function(){
          jQuery('.box2 div').empty();
          jQuery('.box2').removeClass('error_box');
          var error = jQuery(this).parent().children("span.message").text();
          jQuery('.box2').addClass('error_box');
          jQuery('.box2 div').text(error);
        }); 
        jQuery(this).mouseout(function(){
          jQuery('.box2 div').empty();
          jQuery('.box2').removeClass('error_box');
        }); 
      });
         
  });    

</script>


</div>
  



</div>	  

