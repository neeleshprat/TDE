<script language="javascript">
var carrousel = {

		nbSlide : 0,
		nbCurrent : 0,
		timeSlide: 3500,
		elemCurrent : null,
		elem : null,	
		timer : null, 
		
		init : function(elem){
			carrousel.elem = elem;
			
			carrousel.nbSlide=jQuery(elem).find(".news_slide").length -1;

			carrousel.elemCurrent = jQuery(elem).find('.news_slide:first');
			
			/**
			*	On cr?e le timer, Temps en milisecondes
			*/
			$(".show_selected").eq(0).show();
			carrousel.play();	
				
			/**
			*	Stop qd on passe dessus
			*/
			$(elem).hover(carrousel.stop,carrousel.play);

			$(elem).find(".top_3_mini").hover(function(){
				carrousel.gotoSlideManu($(this).index());
			});	 
			
		},
		
		gotoSlideManu : function(int){			
			carrousel.elemCurrent.fadeOut(0);
			$(carrousel.elem).find(".news_slide:eq("+int+")").fadeIn(0);
			carrousel.elemCurrent = jQuery(carrousel.elem).find(".news_slide:eq("+int+")");

			$(carrousel.elem).find(".show_selected").hide();
			$(carrousel.elem).find(".show_selected").eq(int).show();

			carrousel.nbCurrent = int;
		},
	
		next : function(){
					
			carrousel.nbCurrent++;
			
			if (carrousel.nbCurrent>carrousel.nbSlide){ 
				carrousel.nbCurrent = 0; 
			}
			carrousel.gotoSlideManu(carrousel.nbCurrent);
		},
		
		play : function(){
			window.clearInterval(carrousel.timer);
			carrousel.timer = window.setInterval("carrousel.next()",carrousel.timeSlide);
		},
		
		stop : function(){
			window.clearInterval(carrousel.timer);
		}
	}

	jQuery(function(){
		carrousel.init("#news_slideshow");	
	});

</script></div>

