			var myArray = ["reset_all","restaurant_filters_option_photos","restaurant_filters_option_carte","restaurant_filters_option_terrasse","restaurant_filters_option_pets","restaurant_filters_option_parking","restaurant_filters_misc_delivery","restaurant_filters_payment_sodexho","restaurant_filters_option_eurotoque"];

			jQuery("#more_options_select").change(function(e) { 
    			jQuery.each( myArray, function(index, value) { 
					var reset = '#' + value;
   			 	 	jQuery(reset).val(0);
    			});

    			var newSelected = "#" + jQuery('#more_options_select').val();

    			jQuery(newSelected).val(1);
    			
 			});
		</script>
     

