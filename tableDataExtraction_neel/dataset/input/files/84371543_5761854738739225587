<script language="javascript">
function showbanner(a, b, c)
{
c = c.replace("?", "!");
c = c.replace("&", "`");
t1 = "<a target=\"_blank\" href='http://216.194.98.212/site5/fly_banner/click_fly.php?a=" + a + "&b=" + b + "&c=" + c + "&d=" + "" + "&e=" + "" + "' >";
t2 = "<img style=\"max-height=60px;\" src='http://216.194.98.212/site5/fly_banner/show_fly.php?a=" + a + "&b=" + b + "&c=" + c + "&d=" + "" + "&e=" + "" + "' border=\"0\" />";
t3 = "</a>";
document.write(t1+t2+t3);
}
function showbannersame(a, b, c)
{
c = c.replace("?", "!");
c = c.replace("&", "`");
t1 = "<a href='http://216.194.98.212/site5/fly_banner/click_fly.php?a=" + a + "&b=" + b + "&c=" + c + "&d=" + "" + "&e=" + "" + "' >";
t2 = "<img style=\"max-height=60px;\" src='http://216.194.98.212/site5/fly_banner/show_fly.php?a=" + a + "&b=" + b + "&c=" + c + "&d=" + "" + "&e=" + "" + "' border=\"0\" />";
t3 = "</a>";
document.write(t1+t2+t3);
}
function showflash(a, b, c, wid, hei)
{
	showflash_error(a,b,c,wid,hei,'14','/en/en_operadoras/FranceT.asp');
}
/*
AGORA EST? EXTERNO, NO ARQUIVO banner.js
*/

function motrabanner_flash(nome,largura,altura){

document.write('<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000"'+ 
' codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=8,0,0,0"'+
' width="'+largura+'" height="'+altura+'" id="'+nome+'" align="middle">');

document.write('<param name="allowScriptAccess" value="sameDomain" />');

document.write('<param name="movie" value="'+nome+'.swf" />');

document.write('<param name="quality" value="high" />');
			
document.write('<param name="wmode" value="transparent" />');

document.write('<embed src="'+nome+'.swf" quality="high" bgcolor="#ffffff"'+
' width="'+largura+'" height="'+altura+'" name="'+nome+'" align="middle"'+
' allowScriptAccess="sameDomain" type="application/x-shockwave-flash"'+
' pluginspage="http://www.macromedia.com/go/getflashplayer" wmode="transparent" />');

document.write('</object>');

}
