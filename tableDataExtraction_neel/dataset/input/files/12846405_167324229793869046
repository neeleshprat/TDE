<script type="text/javascript">
$(function(){
	$('#climber').mouseover(function(){
		var cl = $('#climber')
		var pos = cl.offset();
		$('#favs').css({ top: pos.top + cl.outerHeight()+1, left: pos.left+2 }).show();
	});
	$('#favs').mouseleave(function(){
		$('#favs').hide();
	});
});
</script>





