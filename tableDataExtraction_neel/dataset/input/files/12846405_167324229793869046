<script type="text/javascript">
function grouper(j,b,a){var n=-1,f="",o,k,i,m,g,e,l,h;if(!j){return""}g=b(j[0]);n=g.length;e=(n+1)/2;l=1;h=e;for(k=0;k<j.length;k++){o=j[k];g=b(o);if(m){h=0;for(i=0;i<=e;i++){if(m[i]===g[i]){h++}}}if(h>e){h=e}else{if(m&&h<l){f+=m.slice(n-l+1,n-h+1).join("");f+=g.slice(h-1,e).join("");h=e}else{f+=g.slice(l-1,h).join("")}}m=g;l=h}f+=g.slice(h,n).join("");return f};

function getTrail(ancestors){
	var trail = '';
	var c=0;
	for(c=0; c<ancestors.length; c++){
		trail += ' > ' + ancestors[c].name;
	}
	return trail.substring(3);
}

$(function(){
	$('#activity_tab').show();
	$('#activity_tab a:first-child').click(function(){
		$('#activity_tab').addClass('selected');
		$('#ascents_tab').removeClass('selected');
		$('#ascents').hide();
		if ($('#activity').length == 0){
			$('#ascents').after('<div id="activity"><div class="waiting" style="height: 5em;margin-bottom: 400px"></div></div>');
			$.ajax({
				dataType: 'json',
				url: "/feed/activity/node/7546063/json?size=30",
				success: function(json){
					var html = '';
					html = grouper(json.activity, function(i){
var a = i.account, p = i.primary.target;
 return Array(
'<div class="account">'+
('1' == a.anonymous
	 ? '<span class="climber"><span class="avatar"><img height="50" width="50" src="/avatar"></span></span> <span class="climber">Someone anonymous</span>'
	 : '<span class="climber">' + ((typeof(a.id) == 'undefined') ? '' : ('<a title="'+a.name+' - Country: '+a.country+' - Ascents: '+a.routesTicked+'" href="/climber/'+a.id+'" class="avatar">')) + '<img height="50" width="50" src="/avatar/'+a.id+'">' + ((typeof(a.id) == 'undefined') ? '' : '</a>') + '</span> <span class="climber">' + ((typeof(a.id) == 'undefined') ? '' : ('<a title="'+a.name+' - Country: '+a.country+' - Ascents: '+a.routesTicked+'" href="/climber/'+a.id+'" >')) + ((typeof(a.name) == 'undefined') ? a.login : a.name) + ((typeof(a.id) == 'undefined') ? '' : '</a>') + '</span>')
,
	'<p>'+
        (typeof(p.type) == 'undefined' ? p.name :
	(p.type == 'route' ? '<span class="gb'+p.gradeBand+'">'+(p.grade || '?')+'</span> ' :'') +
	' <a href="/'+p.type+'/'+p.id+'"'+((typeof(p.ancestors)=='undefined')?'':(' title="'+getTrail(p.ancestors)+'"'))+'>'+
	(p.type == 'route' ? Array(p.stars ? p.stars*1+1 : 0).join('<img src="/static/cids/images/mini-star-1.1.0.png" alt="" />') : '')+
	' '+p.name+'</a> '
//	(p.type == 'area' ? p.subType :'') +
        ) +
	'</p>',
		'<p class="comment">'+i.primary.verbPhrase +' '+p.name+
		(i.secondary ? ' '+i.secondary.verbPhrase +' '+i.secondary.target.name : '')+
		'</p>',
	'',
'</div>'
					)});
					$('#activity').html('<div class="comments">'+html+'</div>');
					$("#activity *[title]").tooltip({ track:true,delay:0,showURL:false,showBody:" - ",fade:250,opacity:1} );
				}
			});
		}
		$('#activity').show();
		return false;
	});
	$('#ascents_tab a:first-child').click(function(){
		$('#activity_tab').removeClass('selected');
		$('#ascents_tab').addClass('selected');
		$('#ascents').show();
		$('#activity').hide();
		return false;
	});
});
</script>








