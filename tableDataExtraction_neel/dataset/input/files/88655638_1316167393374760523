<script type="text/javascript">
var currentDeck = '';
var showDeck = function(deck) {
	if (currentDeck) {
		$(currentDeck + '_link').style.textDecoration = 'underline';
		$(currentDeck).hide();
	}
	$(deck).show();
  $(deck + '_link').style.textDecoration = 'none';
	currentDeck = deck;
}
var showing = 'decks';
var decksOrCabins = function(which) {
  if (showing) {
	  $(showing + '_header').style.textDecoration = 'underline';
		$(showing).hide();
	}
	$(which + '_header').style.textDecoration = 'none';
	$(which).show();
	showing = which;
}
var currentCategory = '';
var showCategory = function(category) {
	if (currentCategory) {
		$(currentCategory + '_link').style.textDecoration = 'underline';
		$(currentCategory).hide();
	}
	$(category).show();
	$(category + '_link').style.textDecoration = 'none';
	currentCategory = category;
}

document.observe('dom:loaded', function() {
	showDeck('Sky');
  showCategory('Deluxe_Verandah_Suite:_S');
  decksOrCabins('decks');
});
</script>
