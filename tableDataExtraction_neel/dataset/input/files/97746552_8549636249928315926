var imageHome		= '/include/menu/images/';
var newimg			= '';
var menu_offset	= 0;
var img_name		= '';
var img_on			= '';
var img_off			= '';

var menuImages	= new Array();

menuImages[0]	 = 'visit_hover.gif';
menuImages[1]	 = 'visit.gif';
menuImages[2]	 = 'explore_hover.gif';
menuImages[3]	 = 'explore.gif';
menuImages[4]	 = 'events_hover.gif';
menuImages[5]	 = 'events.gif';
menuImages[6]	 = 'involved_hover.gif';
menuImages[7]	 = 'involved.gif';
menuImages[8]	 = 'discover_hover.gif';
menuImages[9]	 = 'discover.gif';

function showImage(div, ii) {

	if  ( (ii == 0) || (ii != img_off) ) {
	
		newimg = imageHome + menuImages[ii];

		document.getElementById(div).src=newimg;
	}

}

function onMenu(the_div) {
	var the_style = getStyleObject(the_div);

	the_style.backgroundColor = '#ccc';
	
}

function offMenu(the_div) {
	var the_style = getStyleObject(the_div);

	the_style.backgroundColor = '#ddd';
	
}

function suppressDiv(the_div) {

	var the_style = getStyleObject(the_div);
	the_style.top = "100px";

}

function restoreDiv(the_div) {

	show(the_div);

}

function centerMenu() {

	var current_screen =  document.body.clientWidth || innerWidth;

	var center				= (current_screen / 2);

	var menu_width	= 840;

	var menu_left		= (center - (menu_width / 2)) + 42;

	// This 144 is the width of the left side column that we never want our menu to be to the left of
	
	if (menu_left < 144) {
	
		menu_left = 144;
		
	}

	if (menu_offset) {
		
	menu_left = menu_offset;
	
	}

	hide('m');
	
	var set_left		= 0;
	var left				= menu_left + set_left;

	var newleft		= left + 'px';
	
	var the_style		= getStyleObject('m');
	
	the_style.left		= newleft;

	if (browser == 'ie') {
	
	
	}
	
	show('m');
	
	if (hi_div != 'none') {

		// Derive the image name from the div name
		
		img_name = 'b_' + hi_div;
		
		// Derive the image number from the div name
		
		parts = hi_div.split('m');
		
		img_on = (parts[1] * 2 ) - 2;
		
		img_off = img_on + 1;

		showImage(img_name, img_on);	

	}
}

// -->
</script>


