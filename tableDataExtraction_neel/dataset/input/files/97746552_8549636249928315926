var cookies_on		= 'y';

document.cookie	= 'HasCookie=y;path=/';

if	(document.cookie.indexOf('HasCookie')==(-1)) {

	cookies_on		= 'n';
		
}


//
// Detect flash version
//

var has_flash						= '';
var requiredMajorVersion	= 8;
var requiredMinorVersion	= 0;
var requiredRevision			= 0;

// DetectFlashVer() returns true or false

//
// Get screen dimensions
//

var screen_width		= screen.width;
var screen_height		= screen.height;

has_flash = DetectFlashVer(requiredMajorVersion, requiredMinorVersion, requiredRevision);

//
// Calculate PC Speed
//

var pc_speed	= '';
var start		= new Date();
		
for (var i=0, j=1; i<100000; i++) j++; 
		
var finish		= new Date(); 
pc_speed		= finish - start;

//
// Connection Speed and Connection Type
//

// Finding the connection speed happens in two parts. First we record a start time.	Then we load an image with an onLoad trigger to	record a finish time. That gives us the speed. From that we guess the connection type.

function test_speed() {

	var end = (new Date()).getTime();

	var kbbs = (Math.floor((((fileSize * 8) / ((end - start) / 1000)) / 1024) * 10) / 10);
	
	if (kbbs) {
	
		connectSpeed = kbbs;
		
		if 		(connectSpeed < 15)			{	connectType = 'Slow_Modem';									}
		else if (connectSpeed < 57)		{	connectType = 'Fast_Modem';									}
		else if (connectSpeed < 120)		{	connectType = 'ISDN_Modem';									}
		else											{	connectType = 'partial_T1_or_greater_connection';	}
		
	} else {
	
		connectSpeed	=	'unknown';
		connectType		=	'unknown';

	}

	expire.setTime(today.getTime() + 3600000*24);
	
	document.cookie = 'connectSpeed=' + connectSpeed + ';path=/;expires=' + expire.toGMTString();

	document.cookie = 'connectType=' + connectType + ';path=/;expires=' + expire.toGMTString();
	
}

var connectSpeed		= '';
var connectType		= '';
var fileSize				= 50265;
var start					= (new Date()).getTime();

var image	= '<img src="/images/speedTest.png?t=' + start + '" border="0" height="0" onload="test_speed()">'

document.write(image);

// End of javascript data gathering. Note the onload="test_speed()" in the image var above. The test_speed() function will run only after the test image has been loaded and the script ends there.

// Set php-derived variables

var gv_version		=	'4';
var browser			=	'';
var sub_browser	=	'ccbot';
var browser_ver	=	'1.0';
var os					=	'';
var os_ver				=	'';

// Create cookie variables
	
var newcookie	 	= '';
var today				= new Date();
var expire				= new Date();

expire.setTime(today.getTime() + 3600000*24);

document.cookie = 'gv_version=' +gv_version + ';path=/;expires=' + expire.toGMTString();
document.cookie = 'screen_width=' + screen_width + ';path=/;expires=' + expire.toGMTString();
document.cookie = 'screen_height=' + screen_height + ';path=/;expires=' + expire.toGMTString();
