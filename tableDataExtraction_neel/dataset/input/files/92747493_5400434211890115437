function img_act (imgName)
{
	if (version == "n3")
	{
		imgOn = eval (imgName + "on.src");
		document.getElementById(imgName).src = imgOn;
	}
}
function img_inact (imgName)
{
	if (version == "n3")
	{
		imgOff = eval (imgName + "off.src");
		document.getElementById(imgName).src = imgOff;
	}
}

function deletePhoto(photoNo,s){
	if (document.getElementById('photo'+photoNo).value!="") {
		switch (s){

		case "es":
		alert("Foto "+photoNo+" ser? borrado\n");
		document.getElementById('imagephoto'+photoNo).alt="Foto borrado";
		break;

		case "nl":
		alert("Foto "+photoNo+" zal worden gewist\n");
		document.getElementById('imagephoto'+photoNo).alt="Foto gwist";
		break;

		default:
		alert("Photo "+photoNo+" will be deleted\n");
		document.getElementById('imagephoto'+photoNo).alt="Photo deleted";
		}

		document.getElementById('deletephoto'+photoNo).value="1";
		document.getElementById('imagephoto'+photoNo).src="";
	}
}

