<title>Forwardcharm Fantasy Rally - preview future events.</title>

<link href="/style.css" rel="stylesheet" type="text/css">

<script type="text/javascript" language="javascript">
var msgX;
var msgY;
var msgXstep;
var msgYstep;

function StartSwoop() {
	msgX = 750;
	msgY = 0;
	msgXstep = -3;
	msgYstep = -30;
	setTimeout( 'Swoop()', 40 );
}

function Swoop() {
	if ( msgY <= 0 ) {
		msgY = -msgY;
		msgYstep = -msgYstep - 1;
	}
	msgX += msgXstep;
	msgY += msgYstep;
	msgYstep--;
	document.getElementById( 'whee' ).style.left = msgX;
	document.getElementById( 'whee' ).style.top = msgY;
	document.getElementById( 'whee' ).style.width = 200;

	if ( msgX > -500 ) {
		setTimeout( 'Swoop()', 40 );
	}
}

</script>
