										<script type="text/javascript">
var processForm = function(){
	Effect.BlindDown('captcha_div');

	$('register_button').onclick = transferFieldsAndSubmit;

	return;
}

var transferFieldsAndSubmit = function(){
	var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;

	errors = false;
	
	if($('email').value == 'Insert Email Address' || $('email').value == ''){
		errors = true;
	}
	else if(reg.test($('email').value) == false){
		errors = true;
	}
	
	if(errors == false){
		$('mailingListEmail').value = $('email').value;
		$('mailingListCaptcha').value = $('captcha').value;

		$('submit_step_1').click();
		new Effect.Fade('fade', {duration:0.33, from:1.0, to:0.33});
	}
	else{
		alert('Please enter in a valid email address');
		return false;
	}
}

var submitDetails = function(){
	$('mailingListFirstName').value = $('first_name').value;
	$('mailingListLastName').value = $('last_name').value;
	$('mailingListState').value = $('state').value;
	$('mailingListAge').value = $('age').value;
	$('mailingListMilitary').value = ($('military').checked ? 1 : 0);

	$('submit_step_2').click();
	new Effect.Fade('fade', {duration:0.33, from:1.0, to:0.33});
	return;
}

function highlightEffect(element){
//	new Effect.Highlight(element,{ startcolor: '#ffff99'})
	return;
}
</script>
