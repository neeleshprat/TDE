	<script type="text/javascript">
	    $j(document).ready(function () {

	        /* chapuza de disseny. arreglar esto. Mike.*/
	        $j(".it-cluetip").cluetip({
	            splitTitle: '|', // use the invoking element's title attribute to populate the clueTip...
	            // ...and split the contents into separate divs where there is a "|"
	            showTitle: false, // hide the clueTip's heading
	            arrows: true,
	            cluetipClass: 'cluetip-s2',
	            positionBy: 'bottomTop',
	            showTitle: false,
	            dropShadow: false,
	            width: '180'
	        });



	        $j(".it-sharethis-button").click(function (event) {
	            $j.getScript('http://track.fxstreet.com/trace.aspx?id=8ce74396-b515-40b4-8348-581611610a99&ty=E&st=7fa1772b-d3b2-432a-8cca-1fde7d1cd481&sc=10f1b3c1-13f7-4f93-8bd6-d1610351310b&fr=S');
	        });


	        $j(".it-clksubscribe").click(function (event) {
	            event.preventDefault();
	            var data = $j(this).metadata();
	            var suscribe;
	            if ($j(this).hasClass('btn-s6-enabled')) {
	                suscribe = true;
	            }
	            else {
	                suscribe = false;
	            }
	            $j(this).addClass('btn-loading');
	            $j.ajax({
	                type: 'POST',
	                url: "/_ajax/contentsubscription.aspx",
	                dataType: 'json',
	                data: { idcontingut: data.idcontent, subscribe: suscribe, type: data.type },
	                success: function (json) {
	                    if (json.length > 0) {
	                        var id = "it-clksubscribe-" + json[0];
	                        var link = $j('#' + id);
	                        if (json[1] == 'True') {
	                            link.removeClass('btn-s6-enabled').addClass('btn-s6-disabled');
	                            link.text('Unfollow');
	                            $j("#it-follow-promomsg").show();
	                            link.attr('title', 'Remove from my FX studies');
	                        }
	                        else {
	                            link.addClass('btn-s6-enabled').removeClass('btn-s6-disabled');
	                            link.text('Follow');
	                            link.attr('title', 'Add to my FX studies page');
	                        }
	                        link.removeClass('btn-loading');
	                    }
	                }
	            });
	        });
	        $j("#cmbArhiveHistory").change(function (event) {
	            var url = $j(this).val();
	            if (url == "") return;
	            document.location = url;
	        });
	    });
	    
       
	</script>
