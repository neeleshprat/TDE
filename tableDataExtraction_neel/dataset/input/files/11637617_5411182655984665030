<script type="text/javascript">
<!--
    var intAttributeID = null;
  var intAttributeValueID = null;
    function getProductOptions(iParentProductID, iSubProductID)
  {
    $('#add_to_basket_form').map(function()
    {
      //Build the form vars to pass to the ajax call.
      strURL = '';
      $.each(this.elements, function(i, data)
      {
        if(data.value != '')
        {
          strURL += data.name + '=' + data.value + '&';
        }
      });
    });

    // if any attributes were passd in the page request, add them then disregard
    if(intAttributeID!=null && intAttributeValueID!=null){
      strURL += 'attributes['+intAttributeID+']=' + intAttributeValueID + '&';
      intAttributeID=null;
      intAttributeValueID=null;
    }

    //Call the get_product_options function, passing in form vars
    $.getJSON('/ajax/get_product_options/'+iParentProductID+(iSubProductID != 0 ? '/'+iSubProductID:'')+'?'+strURL,function(data)
    {
      //Unset image id
      if($('.image_id')) $('.image_id').val('0');

      if(data.size_guide_url != '')
      {
        $('#product_size_guide > a').attr('href', data.size_guide_url);
      }

      if(data.lead_times.length > 0)
      {
        //$('#lead_time_min').val();
        //$('#lead_time_max').val();
      }

      //if product is not active then we have no selection so just show message
      if (data.selection.length == 0)
      {
        $('#product_stock_mesage').append("<span>This item is not currently in stock</span>");
        $('#product_stock_mesage').addClass('out_of_stock');
      }

      //Have we got a selection?
      $.each(data.selection, function(i,selection)
      {
        $('.stock_state_positive').hide();
        $('#product_stock_mesage').text('');
        $('#product_stock_mesage').removeClass('discontinued');
        $('#product_stock_mesage').removeClass('out_of_stock');

        if(selection.stock == true && selection.discontinued == false)
        {
          //only show add to basket button if we are showing price (and there is a price), otherwise we don't want to allow customer to purchase
          if (selection.hide_price == false && selection.price_breaks != '') {
            //Add to basket buttons
            if($('.addtobasket_enabled'))
            {
              $('.addtobasket_enabled').show();
              $('.addtobasket_disabled_link').hide();
              $('.addtobasket_disabled').hide();
            }
          }
          //Add to quote buttons
          if($('.addtoquote_enabled'))
          {
            $('.addtoquote_enabled').show();
            $('.addtoquote_disabled_link').hide();
            $('.addtoquote_disabled').hide();
          }

          
          if(selection.low_stock_warning_message)
          {
            $('#low_stock_warning').html(selection.low_stock_warning_message);
            tb_init('a.thickbox, area.thickbox, input.thickbox');
          }


          if(selection.stock_message)
          {
            $('#product_stock_mesage').append("<span class=\"custom_stock_message\" id=\"custom_stock_message_' + selection.stock_message_id + '\">" + selection.stock_message + "</span>");
            if(selection.stock_message_image)
            {
              if($('.stock_state_positive').length == 0)
              {
                $('.stock_delivery').before('<li class="stock_state_positive"></li>');
              }
              $('.stock_state_positive').css('background', 'url(' + selection.stock_message_image + ') no-repeat top left');
              $('.stock_state_positive').show();
            }
            else if(selection.treat_as_in_stock == 'Y')
            {
              $('.stock_state_positive').show();
            }
          }
          else if(selection.str_date_available)
          {
            //Have to specifically say -image or IE doesn't change
            $('.stock_state_positive').css('background-image', '');
            $('#product_stock_mesage').append("<span>We expect stock of this item: " + selection.str_date_available + "</span>");
            $('#product_stock_mesage').addClass('coming_soon');
          }
          else
          {
            //Have to specifically say -image or IE doesn't change
            $('.stock_state_positive').css('background-image', '');
                        $('.stock_state_positive').show();
                      }

        }
        else
        {
          //Remove click handler from the disabled link and remove the messages
          $(".addtobasket_disabled_link").unbind();

          if(selection.discontinued == true)
          {
            if(selection.stock_message)
            {
              $('#product_stock_mesage').append("<span class=\"custom_stock_message\" id=\"custom_stock_message_' + selection.stock_message_id + '\"></span>");
              $('#product_stock_mesage .custom_stock_message').html(decodeURI(selection.stock_message));
              $('#product_stock_mesage').addClass('discontinued');
              if(selection.stock_message_image)
              {
                if($('.stock_state_positive').length == 0)
                {
                  $('.stock_delivery').before('<li class="stock_state_positive"></li>');
                }
                $('.stock_state_positive').css('background', 'url(' + selection.stock_message_image + ') no-repeat top left');
                $('.stock_state_positive').show();
              }
              else if(selection.treat_as_in_stock == 'Y')
              {
                $('.stock_state_positive').show();
              }
            }
            else
            {
              //Discontinued messages
              $('#product_stock_mesage').append("<span>This item has been discontinued</span>");
              $('#product_stock_mesage').addClass('discontinued');
            }

            //Hide all buttons
            $('.addtobasket_enabled').hide();
            $('.addtobasket_disabled_link').hide();
            $('.addtobasket_disabled').hide();
            $('.addtoquote_enabled').hide();
            $('.addtoquote_disabled').hide();
            $('.addtoquote_disabled_link').hide();
          }
          else
          {
            var blnToggleAddToBasket = true;
            var blnToggleAddToQuote = true;
