	<script>
		
		function changeclr(idelm,val,cur,partnerID) {
			switch (partnerID){
				case 36: color1 = "#83c3fa"; color2 = "#DEEBF5";
				break;
				case 11: color1 = "#762216"; color2 = "#DEEBF5";
				break;
				case 206: color1 = "#777777"; color2 = "#DEEBF5";
				break;
				case 207: color1 = "#737373"; color2 = "#DEEBF5";
				break;
				case 208: color1 = "#B3B3B3"; color2 = "#DEEBF5";
				break;
				case 209: color1 = "#015770"; color2 = "#DEEBF5";
				break;
				case 20: color1 = "#008846"; color2 = "#DEEBF5";
				break;
				case 210: color1 = "#00782B"; color2 = "#DEEBF5";
				break;
				case 211: color1 = "#7f252e"; color2 = "#DEEBF5";
				break;
				case 212: color1 = "#017744"; color2 = "#DEEBF5";
				break;
				case 213: color1 = "#5668B4"; color2 = "#DEEBF5";
				break;
				case 215: color1 = "#068fdd"; color2 = "#DEEBF5";
				break;
				case 216: color1 = "#3D3D3D"; color2 = "#DEEBF5";
				break;
				case 217: color1 = "#022c68"; color2 = "#DEEBF5";				
				break;
				default: color1 = "#107FAE"; color2 = "#DEEBF5";
			}
			var elDflt = String('boxmenu_'+idelm)
			var elDflt_1 = String('text_url'+idelm)
			var elDflt_cur = String('boxmenu_'+cur)
			var elDflt_1_cur = String('text_url'+cur)
			if(val==1) {
				document.getElementById(elDflt).style.backgroundColor = color1;
				document.getElementById(elDflt_1).style.color ="#ffffff";
				document.getElementById(elDflt_1).style.fontWeight = "bold";
				document.getElementById(elDflt).style.cursor = 'pointer';
			} else {	
				if(idelm == 126 || idelm == 127 || idelm == 128) {	 document.getElementById(elDflt).style.backgroundColor = color2; } 
				else {	document.getElementById(elDflt).style.backgroundColor ="#ffffff"; }
				document.getElementById(elDflt_1).style.color ="#000000";
				document.getElementById(elDflt_1).style.fontWeight = "normal";
				document.getElementById(elDflt).style.cursor = "default";
			}
			document.getElementById(elDflt_cur).style.backgroundColor = color1;
			document.getElementById(elDflt_1_cur).style.color ="#ffffff";
			document.getElementById(elDflt_1_cur).style.fontWeight = "bold";
		}
		
		function redirect_to_page(url_req)
		{ window.location = url_req;
		}
	</script>

