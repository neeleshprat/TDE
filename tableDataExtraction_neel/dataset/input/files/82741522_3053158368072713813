function fncheckadult()
{
	if (document.fm_search.Pax_Adults.value==0)
		{
			return false;
		}
	return true;
}

function fncheck()
{
	if(document.fm_search.Pax_Infants.value > 0)
		{
			if(document.fm_search.Pax_Adults.value==0 && document.fm_search.Pax_Childern.value == 0)
			{
				return false;
			}
		}
				return true;
}

function fnchildrencheck()
	{
		var children;
		var child_ages;
		var arr, arr_len;

		children = document.fm_search.Pax_Childern.value;
		child_ages = document.fm_search.child_ages.value;
		arr = child_ages.split("")
		arr_len = arr.length;
		if(children > 0)
		{
			if(child_ages == "" || arr_len != children || child_ages == "0")
			{
				return false;
			}
		}
		 return true;
	}
	
function fnroute_category()
{
	var route;
	var route_cat;
	
	route = document.fm_search.route.value;
	route_cat = document.fm_search.Route_Category.value;
	
	if (route == 'ZZZ' && route_cat == 'ANY')
	{
		return false;
	}
	else
	{
		return true;
	}
	
}	

function fntrailercheck()
	
{
	var vehicle;		
	var trailer;
	vehicle = document.fm_search.Vehicle.value;
	trailer = document.fm_search.Trailer.value;
	
	if (trailer != "None")
	{
		if (vehicle == "None")
		{
			return false;
			}
		else
		{
			return true;
			}
	}
	else
	{
		return true;
	}
}
function fnpassengercheck()
{
	var adult,child,infant;
		
	adult = document.fm_search.Pax_Adults.value;
	child = document.fm_search.Pax_Childern.value;
	infant = document.fm_search.Pax_Infants.value;
	
	if (adult == 0 && child == 0 && infant == 0)
	{
		return false;
		}
	else
	{
		return true;
		}
}
function fnsubmit()
{  
  var err, npets;
  var vlength,vheight,vwidth,vFerry Priceight;
  var tlength,theight,twidth,tFerry Priceight,valid;
  var AddChildAges = false;
  var Months = new Array(31,29,31,30,31,30,31,31,30,31,30,31);
  var f = document.fm_search;


		err="";
		//Added by Viral.
		var nchild;
		nchild = document.fm_search.Pax_Childern.value;
		
		if (nchild == 0)
		{
			document.fm_search.child_ages.value = 0;
		}

    if(!(f.Dep_Day.selectedIndex < Months[f.Dep_Month.selectedIndex]))
      err+="Please select a valid departure date.\n\r"
    else
      if(!fndatecheck1())
        err+="Earliest departure date must be 3 days after today.\n\r"
    if(document.fm_search.Ticket_Type.value=="R")
    {
      if(!(f.Rtn_Day.selectedIndex < Months[f.Rtn_Month.selectedIndex]))
        err+="Please select a valid return date.\n\r"
      else
        if(!fndatecheck2())
          err+="Return date must be after departure date. \n\r"
        else
        	valid="Y"
    }
    else
    {
      valid="Y"
    }
	if (!fntrailercheck())
	{
		err += "Vehicle is required to take the Trailer \n\r";
	}
	
	if(document.fm_search.vehicle_make_model.value=="")
		{
			err += "Please enter the vehicle make and model. \n\r";
		}
		
	if(document.fm_search.vehicle_registration.value=="")
		{
			err += "Please enter the vehicle registration number. \n\r";
		}
				
	if (!fnpassengercheck())
	{
		err+="Atleast one adult must travel to book a crossing. \n\r"
	}
	
	if(!fnchildrencheck())
	{
		err+="Please enter ages of all the children \n\r"
    AddChildAges = true;
	}
	if(!fncheck())
	{
		err+="Infants can not travel alone \n\r"
	}
	if (!fncheckadult())
	{
		err+= "There must be atleast one adult to book a crossing \n\r";
	}

	var val_route,a,b,c;
	
	val_route = document.fm_search.route.options[document.fm_search.route.selectedIndex].text;
	document.fm_search.name_route.value = val_route;
	
	//val_route = document.fm_search.Route_Category.options[document.fm_search.Route_Category.selectedIndex].text;
	//document.fm_search.name_route_category.value = val_route;
	document.fm_search.name_vehicle.value=document.fm_search.Vehicle.options[document.fm_search.Vehicle.selectedIndex].text;
	document.fm_search.name_trailer.value=document.fm_search.Trailer.options[document.fm_search.Trailer.selectedIndex].text;
	//document.fm_search.name_route_category.value = "Short Sea";
	
  var Today = new Date();
	a=document.fm_search.Dep_Day.options[document.fm_search.Dep_Day.selectedIndex].text;
	b=document.fm_search.Dep_Month.options[document.fm_search.Dep_Month.selectedIndex].text;
	c=document.fm_search.Dep_Year.options[document.fm_search.Dep_Year.selectedIndex].text;
  if(c < Today.getYear())
    c = Today.getYear();
	document.fm_search.complete_depdate.value=a + " " + b + " " + c
	
	a=document.fm_search.Rtn_Day.options[document.fm_search.Rtn_Day.selectedIndex].text;
	b=document.fm_search.Rtn_Month.options[document.fm_search.Rtn_Month.selectedIndex].text;
	c=document.fm_search.Rtn_Year.options[document.fm_search.Rtn_Year.selectedIndex].text;
  if(c < Today.getYear())
    c = Today.getYear();
	document.fm_search.complete_rtndate.value=a + " " + b + " " + c
	
	if(err!="")
	{
		alert("Please correct the following \n\r \n\r"+err)
    if(AddChildAges)
      fnchildwindow()
	}
	else
	{   fnwait()
		document.fm_search.submit();
	}
}

function fncheckavail()
{
	var outwardf, returnf, s;

	outwardf = document.frm_checkavail.outward_ferry.value;
	returnf = document.frm_checkavail.return_ferry.value;

	//Checking whether the outward departure time and the return departure time are same.

	document.frm_checkavail.check_avail.value = "Y";
	document.frm_checkavail.next_previous.value = "";


		if (document.frm_checkavail.outward_ferry.value == "0")
		{
			alert("Please choose an option for Departure time");
			return;
		}
	
		fncheckavailability_filler()
		document.frm_checkavail.submit();
}
function checkferrycounter(val1,val,d)
{
	document.frm_checkavail.outward_ferry.value = val;
	document.frm_checkavail.record_counter_out.value = val1;
	document.frm_checkavail.outward_ferry_arr_time.value = d;
}
function checkferrycounter1(val1,val,d)
{
	document.frm_checkavail.return_ferry.value = val;
	document.frm_checkavail.record_counter_ret.value = val1;
	document.frm_checkavail.return_ferry_dep_time.value = d;
}
function displayReturn()
{
	var value;
	value = document.fm_search.Ticket_Type.value;
	if (value == "S" )
	{
		document.fm_search.Rtn_Day.disabled = true;
		document.fm_search.Rtn_Time.disabled = true;
		document.fm_search.Rtn_Month.disabled = true;
		document.fm_search.Rtn_Year.disabled = true;
	}
	else
	{
		document.fm_search.Rtn_Day.disabled = false;
		document.fm_search.Rtn_Time.disabled = false;
		document.fm_search.Rtn_Month.disabled = false;
		document.fm_search.Rtn_Year.disabled = false;
	}
}
function fnnext()
{	
	document.frm_checkavail.check_avail.value = "";
	document.frm_checkavail.next_previous.value = "N";
	fnprev_next_filler()
	document.frm_checkavail.submit();
}
function fnprevious()
{
	document.frm_checkavail.check_avail.value = "";
	document.frm_checkavail.next_previous.value ="P";
	fnprev_next_filler()
	document.frm_checkavail.submit();
}
function OnLoad()
{
	window.focus()
	

		var cur_date,cur_day,cur_month,cur_hour,cur_str,new_str
		
		cur_year=document.fm_search.cur_year.value
		cur_day=document.fm_search.cur_day.value
		cur_month=document.fm_search.cur_month.value
		cur_year=document.fm_search.cur_year.value

	if(parseInt(cur_month, 10)==0)
		{
			cur_month=0
		}
	

	for(i=0;i<24;i++)
	{
		if(document.fm_search.Dep_Time.options[i].text==cur_hour)
		{
			document.fm_search.Dep_Time.selectedIndex=i
			break;
		}
	}
	

	for(i=0;i<24;i++)
	{
		if(document.fm_search.Rtn_Time.options[i].text==cur_hour)
		{
			document.fm_search.Rtn_Time.selectedIndex=i
			break;
		}
	}
	

  document.fm_search.Dep_Day.selectedIndex = parseInt(cur_day, 10) - 1
	

  document.fm_search.Rtn_Day.selectedIndex = parseInt(cur_day, 10) - 1
	

  document.fm_search.Dep_Month.selectedIndex = parseInt(cur_month, 10) - 1
	

  document.fm_search.Rtn_Month.selectedIndex = parseInt(cur_month, 10) - 1
	

//	for(i=0;i<2;i++)
//	{
		//if(parseInt(document.fm_search.Dep_Year.options[i].value)==parseInt(cur_year))
//		{
	//		document.fm_search.Dep_Year.selectedIndex=i
		//	break;
//		}
	//}
	

//	for(i=0;i<2;i++)
	//{
		//if(parseInt(document.fm_search.Rtn_Year.options[i].value)==parseInt(cur_year))
//		{
	//		document.fm_search.Rtn_Year.selectedIndex=i
		//	break;
	//	}
//	}		

}
function OpenFiller(msg, autoclose)
{
  open("http://www.introfanco.co.uk/ferry/filler.asp?msg=" + msg + "&close=" + autoclose + "&client_ddi_number=3035","","toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizeable=no,copyhistory=no,width=335,height=360")
}
