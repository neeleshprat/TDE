initialize();
function initialize()
{


  fm_search.Pax_Adults.value = "1";
  fm_search.Pax_Childern.value = "0";
  fm_search.Pax_Infants.value = "0";
  //fm_search.Pax_Pets.value = "0";
  fm_search.Vehicle.value = "CAR";
  fm_search.Trailer.value = "None";
  fm_search.Ticket_Type.value = "R";
  fm_search.child_ages.value = "0"
  fm_search.vheight.value = "150";
  fm_search.vlength.value = "500";


}
