{
  OpenFiller("sessionend", "y")
}
function API84()
{
  OpenFiller("API84", "y")
}
function fnfailure(ddi)
{
  OpenFiller("failuredisplay", "")
}
function fncheckavailfailure(mode, ddi)
{
  OpenFiller("availfail" + mode, "")
}
function fncheckavailability_filler()
{
  OpenFiller("checkavail", "y")
}
function fnprev_next_filler()
{
  OpenFiller("prevnext", "y")
}
function fnfare()
{
  OpenFiller("faredesc", "")
}
function fnterms(terms)
{
  OpenFiller(terms, "")
}
function fnwait()
{
  OpenFiller("searchwait", "y")
}
function fnNoResults_filler()
{
  OpenFiller("noresults", "y")
}
function fnfluidprice()
{
  OpenFiller("fp", "")
}

function fndatecheck2()
{
  var f = document.fm_search;
  var OneDay = 60*1000*60*24;
  var Dep = new Date(f.Dep_Year.value, f.Dep_Month.selectedIndex, f.Dep_Day.selectedIndex + 1);
  var Rtn = new Date(f.Rtn_Year.value, f.Rtn_Month.selectedIndex, f.Rtn_Day.selectedIndex + 1);

    if((Rtn - Dep)/OneDay < 0)
      return false;
    return true;
}

function fndatecheck1()
{
  var f = document.fm_search;
  var Now = new Date();
  var OneDay = 60*1000*60*24;
  var Today = new Date(Now.getYear(), Now.getMonth(), Now.getDate());
  var Dep = new Date(f.Dep_Year.value, f.Dep_Month.selectedIndex, f.Dep_Day.selectedIndex + 1);

    if((Dep - Today)/OneDay < 3)
      return false;
    return true;
}

