function fnroute()
{
	var arrTemp;
	var k

 document.fm_search.route.options.length=0;
 
	for(i=0;i<28;i++)
	{
		arrTemp=arrroute[i+1].split("");
	
			if(document.fm_search.Route_Category.value==arrTemp[0])
		{
			document.fm_search.route.options.length+=1;
			k=document.fm_search.route.options.length;
			document.fm_search.route.options[k-1].value=arrTemp[1];
			document.fm_search.route.options[k-1].text=arrTemp[2];
		} 
	}
}

