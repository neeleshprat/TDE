var minimum_quants = new Object();
function quantChanged(form) {
  var quantity = parseInt(form.cart_quantity.value);
  if (isNaN(quantity)) quantity = 1;
  var denom_id = form.cards_denominations_id.options[form.cards_denominations_id.selectedIndex].value;
  if (minimum_quants[denom_id] > quantity) {
    quantity = minimum_quants[denom_id];
  }
  form.cart_quantity.value = quantity;
}
function denomChanged(form) {
  form.cart_quantity.value = minimum_quants[form.cards_denominations_id.options[form.cards_denominations_id.selectedIndex].value];
}
//--></script>
