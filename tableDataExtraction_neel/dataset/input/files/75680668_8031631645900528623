    <script type=text/javascript>
        IRsearch = [];
        $(document).ready(function() {
            $(".searchField").autocomplete({
                source: function(request, response) {
                    $.ajax({
                        url: "https://copaair.intelliresponse.com/json.jsp",
                        dataType: "jsonp",
                        data: {
                            term: request.term
                        },
                        success: function(data) {
                            response($.map(data, function(item) {
                                return {
                                    label: item.value,
                                    value: item.value
                                }
                            }));
                        }
                    });
                },
                open: function(event, ui) {
                    str = event.target.value;
                    IRsearch = str.split(" ");
                    $(".ui-menu-item a").each(function() {
                        $(this).html(highlightTxt($(this).html()));
                    });
                },
                select: function(event, ui) {
                    setTimeout(function() {
                        $("#questionForm input.searchButton").click();
                    }, 1000);
                },
                minLength: 3
            });
            function highlightTxt(allTheWords) {
                for (var i = 0; i < IRsearch.length; i++) {
                    var regex = new RegExp("(?!<[^<>]*)(" + IRsearch[i].replace(/([\^$\(\)\[\]\{\}\*\.\+\?\;\|\\])/gi, "\$1") + ")(?![^<>]*>)", "gi");
                    allTheWords = allTheWords.replace(regex, "<strong>$1</strong>");
                    allTheWords = allTheWords.replace(/&amp;rsquo;/g, "'")
                }
                return allTheWords;
            }
        })
</script>

