
<div class="well">
	<h1>Classifying {{this.numberOfTables}} tables ...</h1>

	<div class="progress progress-striped active {{this.progressType}}">
		<div class="progress-bar" role="progressbar"
			aria-valuenow="{{this.numberOfCurrentTable}}" aria-valuemin="0"
			aria-valuemax="{{this.numberOfTables}}"
			style="width: {{this.progress">{{this.progress}}%</div>
	</div>

</div>